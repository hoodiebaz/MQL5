
double candle_open_price(ENUM_TIMEFRAMES timeframe, int bars_back)
  {
   return iOpen(_Symbol,timeframe,bars_back);
  }

double candle_high_price(ENUM_TIMEFRAMES timeframe, int bars_back)
  {
   return iHigh(_Symbol,timeframe,bars_back);
  }

double candle_low_price(ENUM_TIMEFRAMES timeframe, int bars_back)
  {
   return iLow(_Symbol,timeframe,bars_back);
  }

double candle_close_price(ENUM_TIMEFRAMES timeframe, int bars_back)
  {
   return iClose(_Symbol,timeframe,bars_back);
  }
